Private Sub t1_Click()
If t1.selectedItem = "基本資料" Then
  基本資料表單1.Visible = True
  座標表單.Visible = False
  技能表單.Visible = False
  對話表單.Visible = False
ElseIf t1.selectedItem = "座標" Then
  基本資料表單1.Visible = False
  座標表單.Left = 基本資料表單1.Left
  座標表單.Top = 基本資料表單1.Top
  座標表單.Visible = True
  技能表單.Visible = False
  對話表單.Visible = False
ElseIf t1.selectedItem = "技能詳細" Then
  基本資料表單1.Visible = False
  技能表單.Left = 基本資料表單1.Left
  技能表單.Top = 基本資料表單1.Top
  座標表單.Visible = False
  技能表單.Visible = True
  對話表單.Visible = False
ElseIf t1.selectedItem = "對話" Then
  基本資料表單1.Visible = False
  對話表單.Left = 基本資料表單1.Left
  對話表單.Top = 基本資料表單1.Top
  座標表單.Visible = False
  技能表單.Visible = False
  對話表單.Visible = True
End If
End Sub



